{
  "name": "unstated",
  "version": "2.1.14",
  "description": "State so simple, it goes without saying",
  "main": "lib/unstated.js",
  "module": "lib/unstated.es.js",
  "types": "lib/unstated.d.ts",
  "repository": "https://github.com/thejameskyle/unstated",
  "author": "James Kyle <me@thejameskyle.com>",
  "license": "SEE LICENSE IN LICENSE",
  "files": [
    "lib"
  ],
  "scripts": {
    "clean": "rm -rf lib",
    "build": "rollup -c && flow-copy-source src lib && cp src/unstated.d.ts lib/unstated.d.ts",
    "typecheck": "flow",
    "test": "jest",
    "format": "prettier --write **/*.{js,json,md}",
    "precommit": "lint-staged",
    "example": "parcel example/index.html",
    "example-ts": "tsc -p tsconfig.example.json && parcel example-ts/index.html",
    "typescript": "tsc -p tsconfig.json",
    "push": "npm publish --registry http://10.0.0.105:4873/"
  },
  "dependencies": {
    "babel-plugin-add-module-exports": "^0.2.1",
    "build": "^0.1.4",
    "create-inferno-context": "https://github.com/Skicom/create-inferno-context/tarball/master",
    "enzyme": "^3.1.0",
    "inferno": "^5.4.2",
    "inferno-server": "5.x"
  },
  "devDependencies": {
    "@babel/plugin-transform-typescript": "^7.0.0-rc.4",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-inferno": "^5.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "babel-register": "^6.26.0",
    "flow": "^0.2.3",
    "flow-bin": "^0.79.1",
    "flow-copy-source": "^1.3.0",
    "husky": "^0.14.3",
    "jest": "^22.1.4",
    "jsdom": "^11.6.2",
    "lint-staged": "^6.1.0",
    "parcel-bundler": "^1.5.1",
    "prettier": "^1.14.2",
    "prop-types": "^15.6.0",
    "rollup": "^0.55.5",
    "rollup-plugin-babel": "^3.0.7",
    "typescript": "^2.9.2"
  },
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
